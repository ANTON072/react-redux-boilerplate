{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --color",
    "build:test": "del build && NODE_ENV=test webpack",
    "build:stg": "del build && NODE_ENV=staging webpack",
    "build:prod": "del build && NODE_ENV=production webpack",
